on boot
    mkdir /persist/rpmb_status
    chmod 0777 /persist/rpmb_status
#for goodix_fp.
    chown system system /dev/goodix_fp

service alipayservice /system/bin/alipayservice
    class late_start
    user system
    group system

service gx_fpd /system/bin/gx_fpd
    class late_start
    user root
    group root system
    disabled
on property:oppo.goodix.fp=1
    start gx_fpd
service fpc1020_sh /system/bin/sh /init.oppo.fingerprints.sh
    class main
    user root
    oneshot
service cryptoengservice /system/bin/cryptoengservice
    class late_start
    user system
    group system

#endif /* VENDOR_EDIT */
#ifdef VENDOR_EDIT
#Yongzhi.Zhang@PSW.MM.AudioDriver.feature, 2017/08/01, add for KTV
    mkdir /data/system/audio 0775 media media
#endif /* VENDOR_EDIT */
    init_user0

    # Reload policy from /data/security if present.
    setprop selinux.reload_policy 1

#ifdef COLOROS_EDIT
#Dubing@Plf.Framework, 2017/01/16, ensure DCIM is exist before restorecon
    mkdir /data/media/0 0770 media_rw media_rw
    mkdir /data/media/0/DCIM 0770 media_rw media_rw
#Dubing@Plf.Framework, 2017/05/02, ensure Camera and Screenshots dir is exist before restorecon
    mkdir /data/media/0/DCIM/Camera 0770 media_rw media_rw
    mkdir /data/media/0/DCIM/Screenshots 0770 media_rw media_rw
#Jiemin.Zhu@PSW.Android.SdardFs, 2017/12/12, Add for sdcardfs delete dcim record
    mkdir /data/.recycle 0770 media_rw media_rw
#endif /* COLOROS_EDIT */

#ifdef VENDOR_EDIT
#Weitao.Chen@PSW.AD.Stability, 2017/08/31, add for dumping hprof when global ref overflow
    mkdir /data/oppo_log 0777 system system
#endif /* VENDOR_EDIT */
#ifdef VENDOR_EDIT

import /init.environ.rc
import /init.usb.rc
import /init.${ro.hardware}.rc
import /init.usb.configfs.rc
import /init.${ro.zygote}.rc

#ifdef VENDOR_EDIT
#Jinke.Zhou@EXP.System.CommonOperatorSoft, 2017/04/28, Add for exp used init.exp.rc
import /init.exp.rc
